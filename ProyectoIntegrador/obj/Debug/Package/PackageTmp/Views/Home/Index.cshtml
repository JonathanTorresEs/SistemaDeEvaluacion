@model IEnumerable<ProyectoIntegrador.Models.Examen>


@{
    ViewBag.Title = "Home Page";
}

@{
    Layout = null;
}

<!DOCTYPE HTML>
<head>
    <title>Inicio</title>
    <link rel="stylesheet" href="~/Content/bootstrap.css">
    <link rel="stylesheet" type="text/css" href="~/Content/resources/css/homepage.css">
</head>
<body>
    <br />
    <h1 style="color:white">Evaluation System</h1>
    <h4 style="color:white">¿Eres alumno o usuario?</h4>

    <!-- Login/ RegisterForm -->
   <div class="container">
        <div class="row">
            <div class="col-md-6 col-md-offset-3">
                <div class="panel panel-login">
                    <div class="panel-body">
                        <div class="row">
                            <div class="col-lg-12">
                   

                    <div class="panel-heading">
                        <div class="row">
                            <div class="col-xs-6 tabs">
                              <a style="font-size:30px;color:white" href="../Alumnoes/login_students">  <span class="glyphicon glyphicon-user" style="font-size: 30px; vertical-align: middle;"> </span> Soy alumno</a>
                            </div>
                            <div class="col-xs-6 tabs">
                                <a style="font-size:30px;color:white" href="../Usuarios/login_users">  <span class="glyphicon glyphicon-user" style="font-size: 30px; vertical-align: middle;"> </span> Soy usuario</a>
                            </div>
                        </div>
                    </div>

                </div>
            </div>
        </div>
    </div>
    <footer>
        <div class="container">
            <div class="col-md-10 col-md-offset-1 text-center">
            </div>
        </div>
    </footer>
    <!-- Login/ RegisterForm end -->

    <script src="~/Content/resources/js/jquery-3.2.1.min.js"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js"></script>
    <script src="~/Content/resources/js/homepage.js"></script>

</body>
</html>



